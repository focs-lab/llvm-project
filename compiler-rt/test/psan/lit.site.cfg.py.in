@LIT_SITE_CFG_IN_HEADER@

config.name_suffix = "@PSAN_TEST_CONFIG_SUFFIX@"
config.psan_lit_source_dir = "@PSAN_LIT_SOURCE_DIR@"
config.has_libcxx = @PSAN_HAS_LIBCXX@
config.apple_platform = "@PSAN_TEST_APPLE_PLATFORM@"
config.apple_platform_min_deployment_target_flag = "@PSAN_TEST_MIN_DEPLOYMENT_TARGET_FLAG@"
config.target_cflags = "@PSAN_TEST_TARGET_CFLAGS@"
config.target_arch = "@PSAN_TEST_TARGET_ARCH@"
config.deflake_threshold = "@PSAN_TEST_DEFLAKE_THRESHOLD@"

# Load common config for all compiler-rt lit tests.
lit_config.load_config(config, "@COMPILER_RT_BINARY_DIR@/test/lit.common.configured")

# Load tool-specific config that would do the real work.
lit_config.load_config(config, "@CMAKE_CURRENT_SOURCE_DIR@/lit.cfg.py")
